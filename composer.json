{
  "name": "cococollective/cococore",
  "description": "CocoCore install profile for Drupal.",
  "type": "drupal-profile",
  "license": "GPL-2.0+",
  "require": {
    "cococollective/coco-article": "7.0.*",
    "cococollective/coco-admin-ux": "7.0.*",
    "cococollective/coco-ux": "7.0.*",
    "wikimedia/composer-merge-plugin": "^1.3"
  },
  "minimum-stability": "dev",
  "prefer-stable": "true",
  "extra": {
    "branch-alias": {
      "dev-7.x-0.x": "7.0.x-dev"
    },
    "merge-plugin": {
      "repositories": [
        {
          "type": "composer",
          "url": "https://packagist.drupal-composer.org"
        },
        {
          "type": "vcs",
          "url": "https://github.com/cococollective/coco-article"
        },
        {
          "type": "vcs",
          "url": "https://github.com/cococollective/coco-admin-ux"
        },
        {
          "type": "vcs",
          "url": "https://github.com/cococollective/coco-ux"
        }
      ]
    }
  },
  "authors": [
    {
      "name": "Sierk Beij",
      "email": "sierk@cococollective.org"
    }
  ]
}
